{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/login",
      "dest": "api/auth/login.js"
    },
    {
      "src": "/api/auth/register",
      "dest": "api/auth/register.js"
    },
    {
      "src": "/api/diaries",
      "dest": "api/diaries/index.js"
    },
    {
      "src": "/api/diaries/(.+)",
      "dest": "api/diaries/[id].js"
    },
    {
      "src": "/api/test",
      "dest": "api/test.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
